page {
  background: #efeff4;
}
.nameul{
  padding: 0 20rpx;
  border-top: 1rpx solid #d9d9d9;
  border-bottom: 1rpx solid #d9d9d9;
  list-style: none;
  margin: 0;
  background: #FFFFFF;
  font-size: 28rpx;
}
.nameli{
  padding: 20rpx 0;
  overflow: hidden;
  position: relative;
  border-bottom: 1rpx solid #D9D9D9;
}
.nameli:last-of-type{
  border-bottom: none;
}
.avatorwrap{
  width: 110rpx;
  height: 110rpx;
  float: left;
  position: relative;
}
.avator{
  width: 110rpx;
    height: 110rpx;
    border-radius: 12rpx;
    float: left;
    background: url("http://statics.myplas.com/upload/16/09/02/logos.jpg") no-repeat;
    background-size: 100% 100%;
    border: 2rpx solid #f88f5f;
    overflow: hidden;
}
.avatorimg{
  width: 110rpx;
  height: 110rpx;
}
.iconV {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    border: 1rpx solid #EEEEEE;
    border-radius: 50%;
    right: -14rpx;
    bottom: -10rpx;
}
.iconV.v1 {
    background: url(http://statics.myplas.com/myapp/img/v1.png) no-repeat;
    background-size: 100% 100%;
}
.iconV.v2 {
    background: url(http://statics.myplas.com/myapp/img/v2.png) no-repeat;
    background-size: 100% 100%;
}
.nameinfo{
  width: auto;
    margin: 0 10rpx 0 130rpx;
    position: relative;
    height: 112rpx;
}
.p{
  font-size: 26rpx;
    line-height: 36rpx;
    margin: 0;
    color: #000;
    width: 560rpx;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
    -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
    -webkit-line-clamp: 1; /** 显示的行数 **/
    overflow: hidden;  /** 隐藏超出的内容 **/
}
.p.first{
    color: #333333;
}
.p.second{
    color: #63769d;
}
.p.third{
    color: #666666;
}
.icon{
  display: inline-block;
    width: 30rpx;
    height: 30rpx;
    position: relative;
    top: 4rpx;
    margin: 0 6rpx;
}
.icon.wxGs {
    background: url(http://statics.myplas.com/myapp/img/wxGs.png) no-repeat;
    background-size: 100% 100%;
}
.icon.wxName {
    background: url(http://statics.myplas.com/myapp/img/wxName.png) no-repeat;
    background-size: 100% 100%;
}
.icon.wxTel {
    background: url(http://pic.myplas.com/myapp/img/wxMobile.png) no-repeat;
    background-size: 100% 100%;
}
.more{ width: 100%; height: 60rpx; line-height: 60rpx; font-size: 32rpx; text-align: center; color: #666666;}