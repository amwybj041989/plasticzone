page{
  height: 100%;
  background: #efeff4;
}
.headlineul{
  display: flex;
  flex-wrap: wrap;
  margin: 0;
  padding: 40rpx 0 0 0;
  border-bottom: 1rpx solid #CCCCCC;
  background: #FFFFFF;
  overflow: hidden;
}
.headlineli{
  width: 20%;
  height: 140rpx;
  margin: 0 0 40rpx 0;
  font-size: 24rpx;
  color: #333333;
  text-align: center;
}

.headlineicon {
    width: 86rpx;
    height: 86rpx;
    display: inline-block;
    margin: 0 0 6rpx 0;
}
