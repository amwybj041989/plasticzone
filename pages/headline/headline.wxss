.headlineul2{
    border-bottom: 1rpx solid #CCCCCC;
    border-top: 1rpx solid #CCCCCC;
    background: #FFFFFF;
    margin: 20rpx 0 0 0;
    padding: 0 20rpx;
}

.headlineli2{
    border-bottom: 1rpx solid #CCCCCC;
    padding: 20rpx 0;
}
.headlineli2:last-of-type{
    border-bottom: none;
}
.title{
    font-size: 30rpx;
    color: #333333;
    font-weight: bold;
    margin: 0 0 10rpx 0;
}
.description,.time{
    font-size: 26rpx;
    color: #999999;
    line-height: 32rpx;    
}
.time{
